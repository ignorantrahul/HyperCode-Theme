        "widget.shadow": "#080a0e66",
        "descriptionForeground": "#8b98a9ff",
        "errorForeground": "#f76769ff",
        "button.background": "#8964e8",
        "button.foreground": "#ffffff",
        "button.hoverBackground": "#a87ffb",
        "button.secondaryBackground": "#333e4f",
        "button.secondaryForeground": "#d9dfe7",
        "button.secondaryHoverBackground": "#475365",
        "input.background": "#1f2939",
        "input.border": "#333e4f",
        "input.foreground": "#d9dfe7",
        "input.placeholderForeground": "#8b98a9b3",
        "inputValidation.errorBorder": "#f76769",
        "inputOption.activeBorder": "#8964e8",
        "inputOption.activeBackground": "#8964e880",
        "inputOption.activeForeground": "#d9dfe7",
        "editor.background": "#171f2b",
        "editor.foreground": "#d9dfe7",
        "editorLineNumber.foreground": "#8b98a9",
        "editorLineNumber.activeForeground": "#ffffff",
        "editor.selectionBackground": "#4d21bb66",
        "editor.selectionHighlightBackground": "#264dcb4d",
        "editor.wordHighlightBackground": "#264dcb4d",
        "editor.wordHighlightStrongBackground": "#264dcb80",
        "editorCursor.foreground": "#8964e8",
        "editorWhitespace.foreground": "#8b98a94d",
        "editorIndentGuide.background": "#333e4f66",
        "editorIndentGuide.activeBackground": "#333e4fb3",
        "statusBar.background": "#1f2939",
        "statusBar.foreground": "#d9dfe7",
        "statusBar.border": "#333e4f",
        "statusBar.debuggingBackground": "#ffa23e",
        "statusBar.debuggingForeground": "#ffffff",
        "statusBar.noFolderBackground": "#1f2939",
        "titleBar.activeBackground": "#171f2b",
        "titleBar.activeForeground": "#d9dfe7",
        "titleBar.inactiveBackground": "#10151d",
        "titleBar.inactiveForeground": "#8b98a9",
        "titleBar.border": "#333e4f",
        "activityBar.background": "#171f2b",
        "activityBar.foreground": "#8964e8",
