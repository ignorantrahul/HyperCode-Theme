        "activityBarBadge.background": "#8964e8",
        "activityBarBadge.foreground": "#ffffff",
        "activityBar.border": "#333e4f",
        "activityBar.activeBorder": "#8964e8",
        "sideBar.background": "#10151d",
        "sideBar.foreground": "#d9dfe7",
        "sideBar.border": "#333e4f",
        "sideBarTitle.foreground": "#d9dfe7",
        "sideBarSectionHeader.background": "#10151d",
        "sideBarSectionHeader.foreground": "#d9dfe7",
        "sideBarSectionHeader.border": "#333e4f",
        "list.activeSelectionBackground": "#333e4f",
        "list.activeSelectionForeground": "#d9dfe7",
        "list.hoverBackground": "#1f2939",
        "list.hoverForeground": "#d9dfe7",
        "list.inactiveSelectionBackground": "#1f2939",
        "list.inactiveSelectionForeground": "#d9dfe7",
        "list.errorForeground": "#f76769",
        "list.warningForeground": "#ffa23e",
        "gitDecoration.modifiedResourceForeground": "#25a6e9",
        "gitDecoration.deletedResourceForeground": "#f76769",
        "gitDecoration.untrackedResourceForeground": "#17b877",
        "gitDecoration.ignoredResourceForeground": "#8b98a9",
        "gitDecoration.conflictingResourceForeground": "#ffa23e"
    },
    "semanticTokenColors": {
        "namespace": "#a87ffbff",
        "class": "#a87ffbff",
        "enum": "#5173f1ff",
        "interface": "#708fffff",
        "struct": "#a87ffbff",
        "typeParameter": "#708fffff",
        "type": "#a87ffbff",
        "parameter": "#d9dfe7ff",
        "variable": "#d9dfe7ff",
        "property": "#d9dfe7ff",
        "enumMember": "#5173f1ff",
        "decorator": "#df8128ff",
        "event": "#708fffff",
        "function": "#708fffff",
        "method": "#708fffff",
        "macro": "#df8128ff",
        "label": "#5173f1ff",
        "comment": "#738295ff",
        "string": "#17975fff",
        "keyword": "#5173f1ff",
        "number": "#df8128ff",
        "regexp": "#df8128ff",
        "operator": "#5173f1ff"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#738295ff",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#17975fff"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#df8128ff"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#5173f1ff"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#708fffff"
            }
        },
        {
            "scope": [
                "variable.language",
                "support.variable",
                "support.constant"
            ],
            "settings": {
                "foreground": "#5173f1ff",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#a87ffbff"
            }
        }
    ]
}